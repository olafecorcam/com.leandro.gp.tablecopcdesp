<?xml version="1.0" encoding="UTF-8"?>
<sdkExtension xmlns="http://www.sap.com/bi/zen/sdk" id="com.leandro.gp.tablecopcdesp"
	title="TableCOPCDESP" version="1.0" vendor="Leandro">
	<component id="TableCOPCDESP" title="TableCOPCDESP" icon="res/table.png"
		handlerType="sapui5"
		propertySheetPath="res/additional_properties_sheet/additional_properties_sheet.html"
		databound="true">
		<jsInclude>res/js/component.js</jsInclude>
		<jsInclude>res/js/numeral.min.js</jsInclude>

		<property id="onclick" type="ScriptText" title="On Change"
			group="Events" />

		<property id="tamanhoCols" type="String" title="Tamanho Colunas"
			group="Events" />
		<property id="topBT" type="String" title="Top Bottom or Top"
			group="Events">
			<possibleValue>TOP</possibleValue>
			<possibleValue>BOTTOM</possibleValue>
		</property>
		<property id="linhasVisiveis" type="int" title="Num Linhas Visiveis"
			group="Events" />

		<property type="ResultSet" group="DataBinding" title="Data Set"
			id="data"></property>

		<initialization>
			<defaultValue property="HEIGHT">25</defaultValue>
			<defaultValue property="WIDTH">250</defaultValue>
			<defaultValue property="topBT">BOTTOM</defaultValue>
			<defaultValue property="linhasVisiveis">5</defaultValue>
			<defaultValue property="tamanhoCols">20px;300px;100px;100px;100px;100px;100px
			</defaultValue>



